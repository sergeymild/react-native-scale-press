  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/react": "^17.0.39",
    "@types/react-native": "0.67.1",
    "pod-install": "^0.1.32",
    "react": "17.0.2",
    "react-native": "0.67.3",
    "typescript": "^4.6.2"
  },
